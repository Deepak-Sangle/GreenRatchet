{
  "workbench.colorTheme": "Default Dark Modern",
  "git.autofetch": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true
  },
  "editor.formatOnSave": true,
  "terminal.integrated.profiles.linux": {
    "zcool": {
      "path": "zsh",
      "args": ["-c", "source ./codespaces/local.zshrc; exec zsh"]
    }
