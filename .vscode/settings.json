    "statusBar.foreground": "#e8f5e9",
    "statusBarItem.hoverBackground": "#3d7a5c",
    "titleBar.activeBackground": "#2d5a4a",
    "titleBar.activeForeground": "#e8f5e9",
    "titleBar.inactiveBackground": "#1a3a2f",
    "titleBar.inactiveForeground": "#a5d6a7"
